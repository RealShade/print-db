server {
        listen 80;
        listen [::]:80;

        server_name dev-print-db.realshade.me;

        return 301 https://$server_name$request_uri;
}

server {
    listen 443 ssl http2;
    listen [::]:443 ssl http2;

    server_name dev-print-db.realshade.me;

    include snippets/print-db.realshade.me.conf;
    include include/print-db/all-*.conf;
}
